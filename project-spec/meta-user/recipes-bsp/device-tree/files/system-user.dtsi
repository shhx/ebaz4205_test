    //         wakeup-source;
    //     };

    //     s3 {
    //         label = "s3";
    //         gpios = <&gpio0 32 1>;
    //         linux,code = <116>;
    //         wakeup-source;
    //     };
    // };

    amba_pl {
        dma_proxy {
            compatible = "xlnx,dma_proxy";
            dma-names = "dma_proxy_rx";
            dmas = <&axi_dma_0 1>;
        };
    };

    intr_tlast {
        compatible = "generic-uio";
        status = "okay";
        interrupt-names = "intr_tlast_name";
        interrupt-parent = <&intc>;
        interrupts = <0 29 1>;
    };

    // reserved-memory {
