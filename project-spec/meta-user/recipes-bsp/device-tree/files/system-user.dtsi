/include/ "system-conf.dtsi"
// /include/ "ebaz4205.dtsi"
/ {
    model = "Zynq EBAZ4205 Board";

    gpio-leds {
        compatible = "gpio-leds";

        led-red {
            label = "led-red";
            gpios = <&gpio0 54 1>;
            default-state = "on";
            linux,default-trigger = "heartbeat";
        };

        led-green {
            label = "led-green";
            gpios = <&gpio0 55 1>;
            default-state = "off";
        };
    };

    // gpio-keys {
